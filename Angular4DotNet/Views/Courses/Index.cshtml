@model string
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="container" ng-init="courses = [
    {number:'CREA101',name:'care of Creatures',instructor:'r hagrid'},
    {number:'DARK502',name:'defence of dark arts',instructor:'Snape'},
    {number:'TRAN201',name:'Transfiguration',instructor:'Mcconagal'}
    ]">
    <div class="row">
        <div class="span10">
            <table class="table table-condensed table-hover">
                <tr>
                    <th>Course</th>
                    <th>Course Name</th>
                    <th>Instructor</th>
                </tr>
                <tr ng-repeat="course in courses">
                    <td>{{course.number}}</td>
                    <td>{{course.name}}</td>
                    <td>{{course.instructor}}</td>    
                </tr>
            </table>
            
        </div>
    </div>
</div>